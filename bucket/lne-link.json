{
    "homepage": "https://github.com/ln-org/lne_link",
    "description": "A custom keyboard configuration manager presented by LNE Lab.",
    "version": "1.3.14",
    "license": "Unknown",
    "notes": [
        "Support list:",
        "https://github.com/ln-org/lne_link/blob/master/SUPPORT.md"
    ],
    "architecture": {
        "64bit": {
            "url": "https://github.com/ln-org/lne_link/releases/download/v1.3.14/LINK.Setup.1.3.14.exe#/dl.7z",
            "hash": "60c26ce76212774669acc052742a8963c36fb4f5e8d1d536c6f89287439d0e5b",
            "pre_install": [
                "Expand-7zipArchive \"$dir\\`$PLUGINSDIR\\app-64.7z\" \"$dir\"",
                "Remove-Item \"$dir\\`$PLUGINSDIR\", \"$dir\\Uninstall*\" -Force -Recurse"
            ]
        }
    },
    "installer": {
        "script": [
            "Import-Module $(Join-Path $(Find-BucketDirectory -Root -Name Scoop4kariiin) scripts/DoradoUtils.psm1)",
            "Mount-ExternalRuntimeData -Source \"$persist_dir\\lne_link\" -Target \"$env:APPDATA\\lne_link\"",
            "Remove-Module -Name DoradoUtils"
        ]
    },
    "uninstaller": {
        "script": [
            "Import-Module $(Join-Path $(Find-BucketDirectory -Root -Name Scoop4kariiin) scripts/DoradoUtils.psm1)",
            "Dismount-ExternalRuntimeData -Target \"$env:APPDATA\\lne_link\"",
            "Remove-Module -Name DoradoUtils"
        ]
    },
    "shortcuts": [
        [
            "LINK.exe",
            "LINK"
        ]
    ],
    "checkver": "github",
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://github.com/ln-org/lne_link/releases/download/v$version/LINK.Setup.$version.exe#/dl.7z"
            }
        }
    }
}
